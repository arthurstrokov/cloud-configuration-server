{
   "id":"spring-boot-cloud-configuration",
   "cmd":null,
   "cpus":0.5,
   "mem":256.0,
   "disk":0,
   "instances":1,
   "labels":{
      "HAPROXY_GROUP":"external"
   },
   "container":{
      "type":"DOCKER",
      "docker":{
         "image":"arthurstrokov/spring-boot-cloud-configuration:latest",
         "forcePullImage":true,
         "network":"BRIDGE"
      },
      "portMappings":[
         {
            "protocol":"tcp",
            "hostPort":0,
            "containerPort":8888,
            "servicePort":8888,
            "name":"cloud-configuration-api"
         }
      ]
   },
   "upgradeStrategy":{
      "minimumHealthCapacity":0,
      "maximumOverCapacity":1
   },
   "healthChecks":[
      {
         "protocol":"COMMAND",
         "command":{
            "value":"curl -f -X GET http://localhost:8888/actuator/health"
         },
         "gracePeriodSeconds":300,
         "intervalSeconds":60,
         "timeoutSeconds":20,
         "maxConsecutiveFailures":3
      },
      {
         "protocol":"COMMAND",
         "command":{
            "value":"pwd"
         },
         "gracePeriodSeconds":300,
         "intervalSeconds":60,
         "timeoutSeconds":20,
         "maxConsecutiveFailures":3,
         "ignoreHttp1xx":false
      }
   ]
}
