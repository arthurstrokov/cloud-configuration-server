{
  "id": "spring-boot-cloud-configuration",
  "cmd": null,
  "cpus": 0.5,
  "mem": 256.0,
  "disk": 0,
  "instances": 1,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "arthurstrokov/spring-boot-cloud-configuration:latest"
    },
    "portMappings": [
      {
        "containerPort": 8888,
        "hostPort": 8888,
        "protocol": "tcp"
      }
    ]
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/actuator/health",
      "gracePeriodSeconds": 3,
      "intervalSeconds": 10,
      "portIndex": 0,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 3
    },
    {
      "protocol": "COMMAND",
      "command": {
        "value": "pwd"
      },
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external"
  }
}
